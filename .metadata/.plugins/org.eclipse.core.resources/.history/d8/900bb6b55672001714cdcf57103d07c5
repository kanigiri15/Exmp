package exampleProj;

import java.util.*;

public class Sol {

	public static List<Integer> list ;
	public static List<Integer> Maxlist;
	private int sum = 0;
	private int largstSum = 0;
	
	
	public static void main(String[] args) {
		list = new ArrayList<>();
		Maxlist = new ArrayList<>();
		Sol s = new Sol();
		Scanner input = new Scanner(System.in);
		int size = input.nextInt();
		int sum = 0;
		int high = 0;
		int largestSum =0;
		int lrgArr[] = new int[5];
		int [][] arr = new int[size][5];
		for(int i=0;i<size;i++){
			for(int j=0;j<5;j++){
				arr[i][j]= input.nextInt();
			}
		}
		//int[][] arr = { { 3000,400,1500,4350,2700 }, { 2050, 3650,650,2750,4300 }, { 2000, 700, 2100,700,1650 }, { 300, 200,500,600, 200 },{3000,3100,3200,1100,1400} };
		Sol sol = new Sol();
		System.out.println(sol.rec(arr,0));
		System.out.println(s.largstSum);
	}
	int rec(int[][] arr,int len){
		for(int i=0;i<5;i++){
			if(list.size() <= 5)
			{
				list.add(len,arr[len][i]);
			}
			else{
				list.set(len,arr[len][i]);				
			}
			if(len<arr.length-1){
				rec(arr,len+1);
			}
			else{
				Iterator itr = list.iterator();
				int high = 0;
				sum = 0;
				while(itr.hasNext()){
					int temp = (int) itr.next();
					if(high < temp){
						sum += temp;
						high = temp;
					}
				}
				if(largstSum < sum){
					largstSum = sum;
					Maxlist = list;
				}
			}
		}
		return largstSum;
	}
	int largSum() {
		return largstSum;
	}
	ArrayList<Integer> largSer() {
		return (ArrayList<Integer>) Maxlist;
	}

}
