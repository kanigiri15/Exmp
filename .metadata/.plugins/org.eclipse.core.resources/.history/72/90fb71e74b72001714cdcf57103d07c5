package exampleProj;

public class exmp {
	
	int a;
	
	public static void main(String[] args) {
		long int1 = 234567678;
		int int2 = (int)int1;
		System.out.println(int2 + " "+ ((Object)int2).getClass());	
		exmp e = new exmp();
		e.a = 10;
		System.out.println(e.a);
		e.print(e.a);
		System.out.println(e.a);
		
	}
	
	public void print(int a) {
		System.out.println(a);
		a= 20;
		System.out.println(a);
	}
}
